@article{ewen:pve2012a,
	Annote = {Explanation of the iteration support within Stratosphere. Stratosphere supports bulk iterations, which work on the complete data set over and over again. They can be thought of as reexecuting a certain path of the parallel dataflow graph again and again. However, the data is usually kept in memory so that an expensive materialization of the data is avoided. Furthermore, the system also supports incremental iterations. Incremental iterations can be used by algorithms which don't have to work on the complete working set in every iteration. Many graph algorithms have this characteristic. By focusing only on the relevant subset of data one can signifcantly speed up the computation.},
	Author = {Ewen, Stephan and Tzoumas, Kostas and Kaufmann, Moritz and Markl, Volker},
	Date-Added = {2014-01-07 16:59:01 +0000},
